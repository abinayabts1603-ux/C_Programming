//OddorEven 2

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    long int n;
    scanf("%ld",&n);
    if(n<0)
        printf("Invalid Input");
    else if(n%2==0){
        printf("Even");
    }else{
        printf("Odd");
    }
    return 0;
}
